<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" >
  <xs:element name="imobiliaria">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="imovel" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="descricao" type="xs:string"/>
              <xs:element name="proprietario" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="nome" type="xs:string"/>
                    <xs:element name="email" type="xs:string" minOccurs="0"/>
                    <xs:element name="telefone" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="endereco" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="rua" type="xs:string"/>
                    <xs:element name="bairro" type="xs:string"/>
                    <xs:element name="cidade" type="xs:string"/>
                    <xs:element name="numero" type="xs:string" minOccurs="0"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="caracteristicas" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="tamanho" type="xs:decimal"/>
                    <xs:element name="numQuartos" type="xs:integer"/>
                    <xs:element name="numBanheiros" type="xs:integer"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="valor" type="xs:decimal"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

</xs:schema>
